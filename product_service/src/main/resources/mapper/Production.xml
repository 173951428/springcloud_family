<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springcloud.product_service.mapper.ProductionMapper">
  <resultMap id="BaseResultMap" type="com.springcloud.product_service.entity.Production">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="price" jdbcType="BIGINT" property="price" />
    <result column="store" jdbcType="BIGINT" property="store" />
  </resultMap>
  <sql id="Base_Column_List">
    id, `price`, store
  </sql>
<select id="findAll" resultMap="BaseResultMap">
  select * from production
</select>

  <select id="findById" parameterType="integer" resultType="com.springcloud.product_service.entity.Production">
    select  * from production where id=#{id}
  </select>
</mapper>